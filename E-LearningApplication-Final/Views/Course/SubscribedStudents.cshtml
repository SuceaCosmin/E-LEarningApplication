@model IEnumerable<E_LearningApplication_Final.Dtos.StudentDto>
@{
    ViewBag.Title = "SubscribedStudents";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Subscribed Students</h2>



<table id="courses" class="table table-bordered table-hover">
    <thead>
        <tr>


            <th>Student Email</th>


        </tr>
    </thead>
    <tbody>

        @{
            foreach (var student in Model)
            {
                <tr>
                    <td>@Html.ActionLink(@student.Email, "Details", "Student", new { id = student.Id }, null)</td>
                  


                </tr>
            }
        }


    </tbody>
</table>

